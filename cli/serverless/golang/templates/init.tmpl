package main

import (
	"fmt"
	"strings"

	"github.com/yomorun/yomo"
)

// Handler will handle the raw data
func Handler(data []byte) (yomo.Tag, []byte) {
	fmt.Printf("sfn received %d bytes\n", len(data))
	output := strings.ToUpper(string(data))
	return yomo.Tag(0x34), []byte(output)
}

func DataTags() []yomo.Tag {
	return []yomo.Tag{0x33}
}
